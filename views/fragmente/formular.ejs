<form id="form_inreg" method="post" class="date" action="<%= locals.action %>" enctype="multipart/form-data">
    <p>
        <label>
            Username: <input type="text" name="username" id="inp-username"
                             value="prof<%= Math.floor(Math.random() * 100000) %>" required pattern="[A-Za-z0-9]+">
        </label>
    </p>
    <p>
        <label>
            Nume: <input type="text" name="nume" id="inp-nume" value="Amariei" required>
        </label>
    </p>
    <p>
        <label>
            Prenume: <input type="text" name="prenume" id="inp-prenume" value="Ionel" required>
        </label>
    </p>
    <p>
        <label>
            Parola: <input id="parl" type="password" name="parola" value="" required>
        </label>
    </p>
    <p id="p-reintrod">
        <label>
            Reintroduceti parola: <input id="rparl" type="password" name="rparola" value="" required>
        </label>
    </p>
    <p>
        <label>
            Email: <input type="text" name="email" id="inp-email" value="example@gmail.com" size="50" required>
        </label>
    </p>
    <p>
        <label>
            Culoare text chat: <select name="culoare_chat" id="sel-culoare">
                <option value="red" selected>rosu</option>
                <option value="green">verde</option>
                <option value="blue">albastru</option>
            </select>
        </label>
    </p>
    <p>
        <label>
            Poza: <input type="file" name="poza">
        </label>
    </p>
    <p>
        <input type="submit" value="Trimite">
        <input type="reset" value="Reseteaza">
    </p>
</form>